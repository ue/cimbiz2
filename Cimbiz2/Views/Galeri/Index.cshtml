@{
    ViewBag.Title = "Galeri";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript" src="http://code.jquery.com/jquery-latest.min.js"></script>


<script>
		$(function() {
			$.ajax({
			type: "GET",
			dataType: "jsonp",
			cache: false,
			url: "https://api.instagram.com/v1/users/242293016/media/recent/?access_token=242293016.1677ed0.ad6842b3fbda4054acfd36c08b935923",
			success: function(data) {
			for (var i = 0; i < 100; i++) {
			$(".instagramapi ul").append("<td><a target='_blank' href='" + data.data[i].link +"'><img src='" + data.data[i].images.low_resolution.url +"' /></a></td>");
			}

			}
			});
			});


</script>

<div class="boxed-layout pane-wrapper clearfix">

		<div id="main-content">	
			<div class="wpv-main layout-full" id="main" role="main">
				<div class="row page-wrapper">
								
					<div class="instagramapi" >
						<ul></ul>
					</div>

				</div>
			</div>

</div> 